{
  "name": "web-starter-nodejs",
  "version": "1.0.0",
  "description": "A starter web application",
  "main": "index.js",
  "scripts": {
    "start": "node dist/bin/www.js",
    "start:watch": "NODE_ENV=development nodemon",
    "build": "NODE_ENV=production tsc && cp -r src/views dist/ && gulp static",
    "lint": "eslint 'src/*.ts' 'src/**/*.ts' 'test/*.ts' 'test/**/*.ts'",
    "lint:fix": "eslint 'src/*.ts' 'src/**/*.ts' 'test/*.ts' 'test/**/*.ts' --fix",
    "test": "DYNAMIC_LP_CERTIFICATE_ORDERS_ENABLED=true DYNAMIC_LLP_CERTIFICATE_ORDERS_ENABLED=true mocha"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@companieshouse/api-sdk-node": "^1.0.61",
    "@companieshouse/node-session-handler": "~4.1.6",
    "ch-structured-logging": "git+ssh://git@github.com/companieshouse/ch-structured-logging-node.git#15a811238ceb58988ab755229a5c07b9500200f2",
    "cookie-parser": "~1.4.4",
    "escape-html": "^1.0.3",
    "express": "~4.17.1",
    "express-validator": "~6.3.0",
    "govuk-frontend": "^3.13.0",
    "http-errors": "~1.7.3",
    "ioredis": "~4.16.0",
    "js-yaml": "^3.14.0",
    "n": "^7.3.1",
    "nunjucks": "~3.2.1",
    "yargs": "~15.3.1"
  },
  "devDependencies": {
    "@types/chai": "~4.2.11",
    "@types/chai-as-promised": "~7.1.2",
    "@types/chai-http": "~4.2.0",
    "@types/cheerio": "~0.22.18",
    "@types/cookie-parser": "~1.4.2",
    "@types/express": "~4.17.6",
    "@types/http-errors": "~1.6.3",
    "@types/ioredis": "~4.14.8",
    "@types/js-yaml": "^3.12.5",
    "@types/mocha": "~7.0.2",
    "@types/node": "~12.7.8",
    "@types/nunjucks": "~3.1.2",
    "@types/sinon": "~9.0.4",
    "@types/sinon-chai": "~3.2.4",
    "@typescript-eslint/eslint-plugin": "2.29.0",
    "@typescript-eslint/parser": "2.29.0",
    "chai": "~4.2.0",
    "chai-as-promised": "~7.1.1",
    "chai-http": "~4.3.0",
    "cheerio": "~1.0.0-rc.3",
    "eslint": "6.8.0",
    "eslint-config-standard": "14.1.1",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "gulp": "~4.0.2",
    "gulp-sass": "~4.0.2",
    "mocha": "~7.2.0",
    "node-sass": "~4.14.1",
    "nodemon": "~2.0.4",
    "sinon": "~9.0.2",
    "sinon-chai": "~3.5.0",
    "ts-node": "~8.8.2",
    "typescript": "~3.8.3"
  },
  "nodemonConfig": {
    "watch": [
      "./src"
    ],
    "ext": "ts,json,html,njk",
    "legacyWatch": true,
    "ignore": [
      "**/*.spec.ts"
    ],
    "exec": "ts-node ./src/bin/www.ts"
  }
}
